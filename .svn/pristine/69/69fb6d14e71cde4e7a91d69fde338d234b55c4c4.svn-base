.gray {
  border: 1px solid #9B9B9B !important;
  color: #9B9B9B !important;
  background-color: #ffffff !important;
}
.item .h_item {
  display: flex;
  align-items: center;
  padding: 32rpx 0;
  background-color: #fff;
  border-top: 1rpx solid rgba(180, 180, 180, 0.2);
}
.item .h_item .h_avar {
  width: 70rpx;
  height: 70rpx;
  border: 1px solid;
  border-radius: 40rpx;
  margin-left: 28rpx;
}
.item .h_item .c_context {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding-left: 30rpx;
  flex-wrap: nowrap;
  flex: 4;
}
.item .h_item .c_context .c_name {
  color: #666666;
  font-size: 28rpx;
  padding-bottom: 7rpx;
}
.item .h_item .c_context .c_text {
  color: #999999;
  font-size: 24rpx;
  padding-top: 7rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 210rpx;
}
.item .h_item .c_btn {
  display: flex;
  font-size: 26rpx;
  color: #E94816;
  margin-left: 114rpx;
}
.item .h_item .c_btn .btn {
  width: 100rpx;
  height: 50rpx;
  text-align: center;
  border: 1px solid #E94816;
  background-color: rgba(233, 72, 22, 0.2);
  line-height: 50rpx;
  margin-left: 16rpx;
  -webkit-border-radius: 8rpx;
  -moz-border-radius: 8rpx;
  border-radius: 8rpx;
}
.item .h_item .c_success {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: flex-end;
  margin-left: 130rpx;
  flex: 4;
  padding-right: 20rpx;
}
.item .h_item .c_success .sign {
  color: #E94816;
  font-size: 26rpx;
}
.item .h_item .c_success .time {
  color: #999999;
  font-size: 24rpx;
}
.fmodal {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.1);
}
.fmodal .f_content {
  background-color: #ffffff;
  width: 80%;
  height: 400rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.fmodal .f_content .title {
  font-size: 34rpx;
  font-weight: 600;
  color: #000;
  text-align: center;
  padding-top: 20rpx;
}
.fmodal .f_content .remark {
  flex: 4;
  padding-top: 10rpx;
  width: 90%;
  margin: 0 auto;
}
.fmodal .f_content .fbtn {
  flex: 2;
  display: flex;
  border-top: 1rpx solid rgba(180, 180, 180, 0.2);
  align-items: center;
}
.fmodal .f_content .fbtn .f_cancel {
  flex: 1;
  text-align: center;
}
.fmodal .f_content .fbtn .f_confirm {
  flex: 1;
  text-align: center;
  border-left: 1rpx solid rgba(180, 180, 180, 0.2);
  color: #E94816;
}
