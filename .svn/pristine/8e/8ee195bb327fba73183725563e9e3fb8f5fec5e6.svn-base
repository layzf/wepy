page {
  background-color: #f7f7f7;
}
.main {
  border-top: 1px solid #F7F7F7;
}
.form-con {
  background-color: #fff;
  padding: 0 28rpx;
}
.form-group {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  height: 108rpx;
  line-height: 108rpx;
  border-bottom: 1px solid #F7F7F7;
}
.form-group > input {
  font-size: 14px;
  color: #333;
}
.border-none {
  border: none;
}
.btn-default {
  margin: 40rpx 28rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  background-color: #ff5d22;
  color: #fff;
  border-color: #ff5d22;
}
