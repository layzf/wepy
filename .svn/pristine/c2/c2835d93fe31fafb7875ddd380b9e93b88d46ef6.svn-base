page {
  background-color: #f7f7f7;
}
.search {
  padding: 44rpx 28rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.search > .flex-row {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  border-radius: 46rpx;
  border: 1px solid #e9e9e9;
  padding: 20rpx;
  padding-bottom: 10rpx;
}
.search .login-icon {
  width: 30rpx;
  height: 36rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
}
.placeholder {
  font-size: 14px;
  color: #666;
}
.audit {
  display: flex;
  align-items: center;
  padding: 40rpx 30rpx;
  justify-content: space-around;
  background-color: #fff;
}
.audit > .audit-icon {
  width: 42rpx;
  height: 42rpx;
}
.audit > .title {
  font-size: 15px;
  color: #333;
  padding-left: 20rpx;
}
