page {
  background-color: #f7f7f7;
}
.head {
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}
.search {
  padding: 44rpx 28rpx;
  background-color: #fff;
  display: flex;
}
.search > .flex-row {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  border-radius: 46rpx;
  border: 1px solid #e9e9e9;
  padding: 20rpx 20rpx 10rpx 20rpx;
  width: 76%;
  font-size: 28rpx;
  background-color: #E9E9E9;
  color: #666666;
}
.search .scan-btn {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 20%;
  font-size: 20rpx;
  color: #333333;
}
.search .scan-btn .scan-img {
  width: 50rpx;
  height: 44rpx;
  padding-bottom: 8rpx;
}
.search .login-icon {
  width: 36rpx;
  height: 42rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
}
.placeholder {
  font-size: 14px;
  color: #666;
}
.title {
  line-height: 108rpx;
  font-size: 14px;
  color: #666;
  padding-left: 28rpx;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}
.list {
  padding: 32rpx 28rpx;
  padding-bottom: 0;
  background-color: #fff;
  display: flex;
  align-content: center;
  border-bottom: 1px solid #eee;
}
.list > .iconImage {
  flex: 1;
  padding-bottom: 30rpx;
}
.list > .iconImage > .icon {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.list .right {
  flex: 7;
  padding-bottom: 30rpx;
}
.list .right > .rightItem {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.list .right > .rightItem > .name {
  font-size: 14px;
  color: #666;
  font-weight: 400;
  padding-left: 30rpx;
}
.list .right > .rightItem > .orange {
  color: #ff5d22;
  font-size: 13px;
  font-weight: 400;
}
.list .right > .rightItem > .bottom {
  color: #999;
  font-size: 12px;
  padding-left: 30rpx;
}
.list .buttonSlef {
  flex: 2;
  border: 1px solid #e94816;
  background-color: rgba(233, 72, 22, 0.1);
  color: #e94816;
  width: 120rpx;
  height: 50rpx;
  border-radius: 8rpx;
  font-size: 13px;
  text-align: center;
  line-height: 50rpx;
}
.list .time {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-end;
}
.list .time .t-status {
  color: #E94816;
  font-size: 13px;
}
.list .time .t-time {
  color: #999999;
  font-size: 12px;
}
.list .over {
  font-size: 13px;
  font-weight: 400;
  color: #4A4A4A;
  line-height: 50rpx;
}
.searchList {
  width: 100%;
  padding: 26rpx 28rpx;
  align-items: center;
  z-index: 999;
}
.searchList > .right {
  padding-bottom: 0rpx;
}
.searchList > .iconImage {
  padding-bottom: 0rpx;
}
.f_btn {
  flex: 4;
}
.f_btn .btn {
  padding: 10rpx 0;
  text-align: center;
  border: 1px solid #E94816;
  background-color: rgba(233, 72, 22, 0.2);
  margin-left: 16rpx;
  -webkit-border-radius: 8rpx;
  -moz-border-radius: 8rpx;
  border-radius: 8rpx;
  font-size: 26rpx;
  color: #E94816;
  width: 120rpx;
}
